{
  "name": "client-springreen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lazyy": "sass  --no-source-map styles/index.scss:styles/index.css",
    "watch:lazyy": "sass  --no-source-map styles/index.scss:styles/index.css --watch",
    "purge:lazyy": "purgecss --css styles/index.css --content \"pages/**/*.js\" \"components/**/*.js\" --output styles/",
    "build:lazyy": "npm run purge:lazyy && postcss styles/index.css -u autoprefixer cssnano -r --no-map"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@stripe/react-stripe-js": "^1.13.0",
    "@stripe/stripe-js": "^1.42.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.12",
    "firebase": "^9.10.0",
    "framer-motion": "^6.5.1",
    "lottie-react": "^2.3.1",
    "next": "^12.3.1",
    "postcss-cli": "^10.0.0",
    "purgecss": "^4.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.54.0",
    "stripe": "^10.15.0"
  },
  "devDependencies": {
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3"
  }
}
